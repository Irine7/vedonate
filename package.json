{
	"name": "vedonate",
	"version": "1.0.0",
	"private": true,
	"description": "VeDonate - Decentralized blood donation platform on VeChain",
	"scripts": {
		"build": "next build",
		"clean": "rm -rf .next dist .turbo",
		"dev": "next dev --turbopack",
		"install:clean": "pnpm install --frozen-lockfile",
		"lint": "next lint",
		"type-check": "tsc --noEmit",
		"contracts:compile": "cd contracts && npm run compile",
		"contracts:deploy:testnet": "cd contracts && npm run deploy:testnet",
		"contracts:deploy:mainnet": "cd contracts && npm run deploy:mainnet",
		"setup:contracts": "cd contracts && npm install"
	},
	"packageManager": "pnpm@8.15.0",
	"dependencies": {
		"@chakra-ui/anatomy": "2.3.4",
		"@chakra-ui/react": "2.8.2",
		"@emotion/react": "^11.13.5",
		"@emotion/styled": "^11.13.5",
		"@tanstack/react-query": "^5.64.2",
		"@vechain/vechain-contract-types": "1.0.3",
		"@vechain/vechain-kit": "^2.0.0-rc.16",
		"@vechain/dapp-kit-react": "^2.0.0",
		"@vechain/sdk-core": "^2.0.0",
		"@vechain/sdk-network": "^2.0.0",
		"i18next": "^24.2.1",
		"i18next-browser-languagedetector": "^8.0.2",
		"mixpanel-browser": "^2.61.1",
		"next": "^15.2.3",
		"react": "^18",
		"react-dom": "^18",
		"react-i18next": "^15.4.0"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "^14.1.4",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^9.12.0",
		"eslint-config-next": "14.1.4",
		"typescript": "5.3.3"
	},
	"homepage": "https://vedonate.vechain.org",
	"keywords": [
		"vechain",
		"blockchain",
		"donation",
		"blood",
		"nft",
		"defi",
		"vedonate"
	]
}
